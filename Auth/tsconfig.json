{
    "compilerOptions": {
        "lib": [
            "ES5",
            "ES6",
            "DOM"
        ],
        "target": "ES6",
        "module": "CommonJS",
        "moduleResolution": "node",
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "rootDir": "src",
        "outDir": "dist",
        "esModuleInterop": true
    },
    "exclude":[
      "./node_modules"
    ]
}